{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "images"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "cosmos",
      "databaseName": "azure101",
      "collectionName": "images",
      "connectionStringSetting": "perguscosmos_DOCUMENTDB"
    }
  ],
  "scriptFile": "../dist/func-http-post-images/index.js"
}
